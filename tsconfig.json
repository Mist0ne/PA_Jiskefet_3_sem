{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"lib": [
			"es2015",
			"dom"
		],
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"suppressImplicitAnyIndexErrors": true,
		"preserveConstEnums": true,
		"removeComments": false,
		"noLib": false,
		"declaration": false,
		"sourceMap": true,
		"strictNullChecks": true,
		"noImplicitThis": true,
        "experimentalDecorators": true,
        "jsx": "react",
        "jsxFactory": "m",
        "noUnusedLocals": true,
        "forceConsistentCasingInFileNames": true
	}
}	